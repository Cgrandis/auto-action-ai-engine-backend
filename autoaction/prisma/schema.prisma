generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-autoaction"
}

datasource db {
  provider = "postgresql"
  url      = env("AUTOACTION_DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  from      String
  to        String
  direction String
  body      String
  createdAt DateTime @default(now())
}

model Contact {
  id        Int      @id @default(autoincrement())
  number    String   @unique
  name      String
  createdAt DateTime @default(now())
}

model Example {
  id          Int      @id @default(autoincrement())
  profession  String
  title       String
  description String
  createdAt   DateTime @default(now())
}

